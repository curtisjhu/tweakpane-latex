{
  "name": "tweakpane-latex",
  "version": "0.1.0",
  "description": "Tweakpane plugin for displaying Latex (MathJax or Katex) in panels",
  "main": "dist/tweakpane-latex.js",
  "types": "dist/types/index.d.ts",
  "author": "Curtis Hu",
  "license": "MIT",
  "keywords": [
    "tweakpane",
    "tweakpane-plugin",
    "plugin",
    "latex",
    "mathjax",
    "katex",
    "description",
    "text",
    "markdown"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/curtisjhu/tweakpane-latex.git"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "prepare": "run-s clean build",
    "prepublishOnly": "npm test",
    "start": "npm run watch",
    "test": "eslint --ext .ts \"src/**/*.ts\"",
    "assets": "run-s clean build assets:version assets:zip",
    "assets:version": "node scripts/assets-append-version.js",
    "assets:zip": "zip -x \"*types*\" -j -r $(cat package.json | npx json name)-$(cat package.json | npx json version).zip dist",
    "clean": "rimraf dist *.tgz *.zip",
    "build": "run-p build:*",
    "build:dev": "rollup --config rollup.config.js",
    "build:dts": "tsc --project src/tsconfig-dts.json",
    "build:prod": "rollup --config rollup.config.js --environment BUILD:production",
    "format": "run-p format:*",
    "format:scss": "prettier --parser scss --write \"src/sass/**/*.scss\"",
    "format:ts": "eslint --ext .ts --fix \"src/**/*.ts\"",
    "watch": "run-p watch:*",
    "watch:sass": "onchange --initial --kill \"src/sass/**/*.scss\" -- npm run build:dev",
    "watch:ts": "onchange --initial --kill \"src/**/*.ts\" -- rollup --config rollup.config.js"
  },
  "dependencies": {
    "katex": "^0.16.9",
    "marked": "^9.1.2",
    "marked-code-format": "^1.0.1",
    "marked-katex-extension": "^4.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.1",
    "@rollup/plugin-typescript": "^8.2.0",
    "@tweakpane/core": "^1.0.6",
    "@types/prettier": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "autoprefixer": "^10.2.4",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss": "^8.2.6",
    "rimraf": "^3.0.2",
    "rollup": "^2.39.1",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.49.9",
    "typescript": "^4.1.5"
  },
  "peerDependencies": {
    "tweakpane": "^3.1.9"
  }
}
